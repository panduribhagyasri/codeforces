13.https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/M
A:#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    char b;
    cin>>b;
    int ascii=(int)b;
    if(97<=ascii)
    {
        cout<<"ALPHA"<<endl;
        cout<<"IS SMALL";
    }
    else if(ascii>=65)
    {
        cout<<"ALPHA"<<endl;
        cout<<"IS CAPITAL";
    }
    else if(ascii>=48)
    {
        cout<<"IS DIGIT";
    }
}
.............................................................................................
14.https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/N
A:#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    char b;
    cin>>b;
    int ascii=(int)b;
    if(ascii>=97)
    {
        int c=ascii-32;
        char val=(char)c;
        cout<<val<<endl;
    }
    else
    {
        int c=ascii+32;
        char val=(char)c;
        cout<<val<<endl;
    }
    
}
.............................................................................................
15.https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/O
A:
#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int a,b;
    char c;
    cin>>a>>c>>b;
    if(c=='+')
    {
        cout<<a+b;
    }
    if(c=='-')
    {
        cout<<a-b;
    }
    if(c=='*')
    {
        cout<<a*b;
    }
    if(c=='/')
    {
        cout<<a/b;
    }
    
}
.............................................................................................

16.https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/P
A:#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int a,b;
    cin>>a;
    while(a>=10)
    {
        a/=10;
        
    }
    if(a%2==0)
    {
        cout<<"EVEN";
    }
    else
    {
        cout<<"ODD";
    }
    
}
