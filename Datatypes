#include <iostream>
#include <cstdio>
#include<iomanip>
using namespace std;

int main() {
    
    int64_t a,b,c,d,e;
    cin>>a>>b>>c>>d;
    e=(a*b)-(c*d);
    cout<<"Difference = "<<e;

    return 0;
}
