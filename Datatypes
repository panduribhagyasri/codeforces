3.https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/C
A.
#include<iostream>
using namespace std;
int main()
{
    int64_t a,b;
    cin>>a>>b;
    cout<<a <<" + "<<b<<" = "<<a+b<<"\n";
    cout<<a <<" * "<<b<<" = "<<a*b<<"\n";
    cout<<a <<" - "<<b<<" = "<<a-b;
    
}
