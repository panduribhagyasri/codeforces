11.https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/K
A:#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int a,b,c,d,e;
    cin>>a>>b>>c;
    d=max(a,max(b,c));
    e=min(a,min(b,c));
    cout<<e<<" "<<d;
}
