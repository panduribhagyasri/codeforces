7.https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/G
A.
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
     int64_t n;
     //n is the range given to add all number in between 1 to n
     cin>>n;
     int64_t sum=0;
     //for adding the sum of elements from 1to n
     for(int i=0;i<=n;i++)
     {
         sum=sum+i;
     }
     cout<<sum;
     
}
